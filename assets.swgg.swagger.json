{
    "basePath": "/",
    "externalDocs": {
        "description": "find out more about facebook's web-apis",
        "url": "https://developers.facebook.com/docs/"
    },
    "info": {
        "description": "this zero-dependency package will provide a javascript-client for facebook's web-apis, with a working web-demo",
        "title": "swgg-facebook",
        "version": "2017.11.6",
        "x-swgg-downloadStandaloneApp": "https://kaizhu256.github.io/node-swgg-facebook/build..beta..travis-ci.org/app/assets.app.js",
        "x-swgg-homepage": "https://github.com/kaizhu256/node-swgg-facebook"
    },
    "parameters": {
        "facebookGraphAccessTokenApp": {
            "default": "xxxxxxxx",
            "description": "App access tokens are used to make requests to Facebook APIs on behalf of an app rather than a user. This can be used to modify the parameters of your app, create and manage test users, or read your apps's insights.\n\nhttps://developers.facebook.com/docs/facebook-login/access-tokens/#apptokens\n\nhttps://developers.facebook.com/tools/accesstoken/",
            "in": "query",
            "name": "access_token",
            "required": true,
            "type": "string",
            "x-swgg-tags0": "facebook-graph"
        },
        "facebookGraphAccessTokenPage": {
            "default": "xxxxxxxx",
            "description": "Page access tokens are used in Graph API calls to manage Facebook Pages. To generate a page access token, an admin of the page must grant an extended permission called manage_pages.\n\nhttps://developers.facebook.com/docs/facebook-login/access-tokens/#pagetokens\n\nhttps://developers.facebook.com/tools/accesstoken/",
            "in": "query",
            "name": "access_token",
            "required": true,
            "type": "string",
            "x-swgg-tags0": "facebook-graph"
        },
        "facebookGraphAccessTokenUser": {
            "default": "xxxxxxxx",
            "description": "The user token is the most commonly used type of token. This kind of access token is needed any time the app calls an API to read, modify or write a specific person's Facebook data on their behalf. User access tokens are generally obtained via a login dialog and require a person to permit your app to obtain one.\n\nhttps://developers.facebook.com/docs/facebook-login/access-tokens/#usertokens\n\nhttps://developers.facebook.com/tools/accesstoken/",
            "in": "query",
            "name": "access_token",
            "required": true,
            "type": "string",
            "x-swgg-apiKey": true,
            "x-swgg-tags0": "facebook-graph"
        },
        "facebookGraphTestUserId": {
            "description": "A test user associated with a Facebook app. Test users are created and associated using the /{app-id}/accounts/test-users edge or in the App Dashboard.\n\nhttps://developers.facebook.com/docs/apps/test-users#managetool\n\nhttps://developers.facebook.com/apps/{app-id}/roles/test-users/",
            "in": "path",
            "name": "test-user-id",
            "required": true,
            "type": "string",
            "x-swgg-tags0": "facebook-graph"
        },
        "facebookGraphUserId": {
            "default": "me",
            "description": "A user represents a person on Facebook. The /{user-id} node returns a single user.\n\nhttps://developers.facebook.com/docs/graph-api/reference/user/",
            "in": "path",
            "name": "user-id",
            "required": true,
            "type": "string",
            "x-swgg-tags0": "facebook-graph"
        },
        "facebookGraphVersion": {
            "default": "v2.10",
            "description": "The Graph API has multiple versions available to access at any one time. Each version contains a set of core fields and edge operations. We guarantee that those core APIs will be available and un-modified in that version for at least 2 years from release. The platform changelog can tell you which versions are currently available.\n\nhttps://developers.facebook.com/docs/graph-api/overview/#versions",
            "in": "path",
            "name": "facebook_graph_version",
            "required": true,
            "type": "string",
            "x-swgg-tags0": "facebook-graph"
        }
    },
    "paths": {
        "/oauth/access_token": {
            "get": {
                "description": "App access tokens are used to make requests to Facebook APIs on behalf of an app rather than a user. This can be used to modify the parameters of your app, create and manage test users, or read your apps's insights.\n\nhttps://developers.facebook.com/docs/facebook-login/access-tokens/#apptokens",
                "operationId": "_2Foauth_2Faccess_token_20get",
                "parameters": [
                    {
                        "description": "https://developers.facebook.com/apps/",
                        "in": "query",
                        "name": "client_id",
                        "required": true,
                        "type": "string"
                    },
                    {
                        "description": "https://developers.facebook.com/apps/",
                        "in": "query",
                        "name": "client_secret",
                        "required": true,
                        "type": "string"
                    },
                    {
                        "default": "client_credentials",
                        "description": "https://developers.facebook.com/apps/",
                        "in": "query",
                        "name": "grant_type",
                        "required": true,
                        "type": "string"
                    }
                ],
                "responses": {
                    "default": {
                        "description": "default response"
                    }
                },
                "summary": "App Access Tokens.",
                "tags": [
                    "facebook-graph"
                ],
                "x-swgg-host": "graph.facebook.com",
                "x-swgg-operationIdFromPath": true
            }
        },
        "/{facebook_graph_version}/{achievement-id}#requireAchievementId": {
            "get": {
                "description": "Represents a user gaining an achievement in a Facebook game. Not to be confused with an achievement type.\n\nhttps://developers.facebook.com/docs/graph-api/reference/achievement/",
                "operationId": "_2F_7Bfacebook_graph_version_7D_2F_7Bachievement-id_7D_23requireAchievementId_20get",
                "parameters": [
                    {
                        "$ref": "#/parameters/facebookGraphAccessTokenUser"
                    },
                    {
                        "$ref": "#/parameters/facebookGraphVersion"
                    },
                    {
                        "description": "ID of this particular achievement.",
                        "in": "path",
                        "name": "achievement-id",
                        "required": true,
                        "type": "string",
                        "x-swgg-tags0": "facebook-graph"
                    }
                ],
                "responses": {
                    "default": {
                        "description": "default response"
                    }
                },
                "summary": "Achievement /{achievement-id}. Reading.",
                "tags": [
                    "facebook-graph"
                ],
                "x-swgg-host": "graph.facebook.com",
                "x-swgg-operationIdFromPath": true,
                "x-swgg-tags0": "facebook-graph"
            }
        },
        "/{facebook_graph_version}/{test-user-id}#requireTestUserId": {
            "delete": {
                "description": "A test user associated with a Facebook app. Test users are created and associated using the /{app-id}/accounts/test-users edge or in the App Dashboard. You can delete a test user by making a delete operation on this node.\n\nhttps://developers.facebook.com/docs/graph-api/reference/test-user/",
                "operationId": "_2F_7Bfacebook_graph_version_7D_2F_7Btest-user-id_7D_23requireTestUserId_20delete",
                "parameters": [
                    {
                        "$ref": "#/parameters/facebookGraphAccessTokenApp"
                    },
                    {
                        "$ref": "#/parameters/facebookGraphVersion"
                    },
                    {
                        "$ref": "#/parameters/facebookGraphUserId"
                    }
                ],
                "responses": {
                    "default": {
                        "description": "default response"
                    }
                },
                "summary": "Test User /{test-user-id}. Deleting.",
                "tags": [
                    "facebook-graph"
                ],
                "x-swgg-host": "graph.facebook.com",
                "x-swgg-operationIdFromPath": true,
                "x-swgg-tags0": "facebook-graph"
            },
            "post": {
                "consumes": [
                    "application/x-www-form-urlencoded"
                ],
                "description": "A test user associated with a Facebook app. Test users are created and associated using the /{app-id}/accounts/test-users edge or in the App Dashboard. You can publish to this node to update the test user's password or name.\n\nhttps://developers.facebook.com/docs/graph-api/reference/test-user/",
                "operationId": "_2F_7Bfacebook_graph_version_7D_2F_7Btest-user-id_7D_23requireTestUserId_20post",
                "parameters": [
                    {
                        "$ref": "#/parameters/facebookGraphAccessTokenApp"
                    },
                    {
                        "$ref": "#/parameters/facebookGraphVersion"
                    },
                    {
                        "$ref": "#/parameters/facebookGraphUserId"
                    },
                    {
                        "description": "New name for the test user.",
                        "in": "formData",
                        "name": "name",
                        "type": "string"
                    },
                    {
                        "description": "A new password for the test user.",
                        "in": "formData",
                        "name": "password",
                        "type": "string"
                    }
                ],
                "responses": {
                    "default": {
                        "description": "default response"
                    }
                },
                "summary": "Test User /{test-user-id}. Publishing and Updating.",
                "tags": [
                    "facebook-graph"
                ],
                "x-swgg-host": "graph.facebook.com",
                "x-swgg-operationIdFromPath": true,
                "x-swgg-tags0": "facebook-graph"
            }
        },
        "/{facebook_graph_version}/{user-id}#requireUserId": {
            "get": {
                "description": "A user represents a person on Facebook. The /{user-id} node returns a single user.\n\nhttps://developers.facebook.com/docs/graph-api/reference/user/",
                "operationId": "_2F_7Bfacebook_graph_version_7D_2F_7Buser-id_7D_23requireUserId_20get",
                "parameters": [
                    {
                        "$ref": "#/parameters/facebookGraphAccessTokenUser"
                    },
                    {
                        "$ref": "#/parameters/facebookGraphVersion"
                    },
                    {
                        "$ref": "#/parameters/facebookGraphUserId"
                    },
                    {
                        "collectionFormat": "csv",
